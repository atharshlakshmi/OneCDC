# Server Configuration
PORT=5000
NODE_ENV=development

# Database
MONGODB_URI=mongodb://localhost:27017/onecdc

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRE=7d

# External APIs
SINGPASS_API_URL=https://mock-singpass-api.example.com
CORPPASS_API_URL=https://mock-corppass-api.example.com
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
REVIEW_RATE_LIMIT_MAX=5
REVIEW_RATE_LIMIT_WINDOW_MS=3600000
REPORT_RATE_LIMIT_MAX=10
REPORT_RATE_LIMIT_WINDOW_MS=3600000

# Default Location (Marine Parade, Singapore)
DEFAULT_LAT=1.3016
DEFAULT_LNG=103.9056

# File Upload
MAX_FILE_SIZE=5242880
MAX_FILES=5

# CORS
FRONTEND_URL=http://localhost:3000

# Thresholds
SHOPPER_WARNING_THRESHOLD=3
OWNER_REPORT_THRESHOLD=5
